	};

	hsi2c_13_bus_in: hsi2c_13-bus-in {
		samsung,pins = "gpm2-0", "gpm3-0";
		samsung,pin-function = <3>;
		samsung,pin-pud = <3>;
		samsung,pin-drv = <0>;
	};

	/* REAR2 */
	/* No pull-up resistor, use internal one */
	hsi2c_15_bus: hsi2c_15-bus {
		samsung,pins = "gpm6-0", "gpm7-0";
		samsung,pin-function = <3>;
		samsung,pin-pud = <3>;
		samsung,pin-drv = <0>;
	};

	hsi2c_15_bus_in: hsi2c_15-bus-in {
		samsung,pins = "gpm6-0", "gpm7-0";
		samsung,pin-function = <3>;
		samsung,pin-pud = <3>;
		samsung,pin-drv = <0>;
	};
};

