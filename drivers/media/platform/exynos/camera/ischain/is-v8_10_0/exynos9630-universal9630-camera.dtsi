		samsung,pin-drv = <0>;
	};

	hsi2c_13_bus_in: hsi2c_13-bus-in {
		samsung,pins = "gpm2-0", "gpm3-0";
		samsung,pin-function = <0>;
		samsung,pin-pud = <0>;
		samsung,pin-drv = <0>;
	};

	/* FRONT, REAR CAM2, REAR CAM3 */
	hsi2c_15_bus: hsi2c_15-bus {
